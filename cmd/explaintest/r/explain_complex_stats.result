drop table if exists dd;
CREATE TABLE dd (
id int(11) UNSIGNED NOT NULL,
aid varchar(35) NOT NULL comment '[[set=cn.sbkcq,us.sbkcq]]',
pt varchar(10) NOT NULL comment '[[set=android,ios]]',
