└─Selection	cop[tikv]		lt(generated_columns.sgc.b, 3)
  └─IndexRangeScan	cop[tikv]	table:sgc, index:idx_a_b(a, b)	range:[-inf,3), keep order:false, stats:pseudo
DROP TABLE IF EXISTS sgc1,
sgc2;
CREATE TABLE `sgc1` (
