  `col_1` text NOT NULL,
  PRIMARY KEY (`col_1`(3)) /*T![clustered_index] NONCLUSTERED */,
  KEY `idx_2` (`col_1`(5)),
  UNIQUE KEY `idx_3` (`col_1`(5)),
  KEY `idx_4` (`col_1`(4))
