prepare stmt1 from 'select * from t where a = ?;';
prepare stmt2 from 'select * from t where a in (?, ?);';
set @a=-1.56018e38, @b=-1.96716e38, @c=9.46347e37, @d=1.1, @e=0, @f=-1, @g=1, @h=2, @i=-1.1;
