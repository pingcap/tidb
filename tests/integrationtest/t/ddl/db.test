# TestCreateTextAdjustLen
# Close issue #27862 https://github.com/pingcap/tidb/issues/27862
# Ref: https://dev.mysql.com/doc/refman/8.0/en/storage-requirements.html#data-types-storage-reqs-strings
# text(100) in utf8mb4 charset needs max 400 byte length, thus tinytext is not enough.
drop table if exists t;
create table t(a text(100));
show create table t;
alter table t add b text(100);
alter table t add c text;
alter table t add d text(50);
alter table t change column a a text(50);
show create table t;
# Ref: https://dev.mysql.com/doc/refman/8.0/en/storage-requirements.html
# TINYBLOB, TINYTEXT	L + 1 bytes, where L < 2^8
# BLOB, TEXT	L + 2 bytes, where L < 2^16
# MEDIUMBLOB, MEDIUMTEXT	L + 3 bytes, where L < 2^24
# LONGBLOB, LONGTEXT	L + 4 bytes, where L < 2^32
alter table t change column d d text(100);
alter table t change column c c text(30000);
alter table t change column b b text(10000000);
show create table t;
drop table if exists t;

