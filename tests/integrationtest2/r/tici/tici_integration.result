CREATE DATABASE IF NOT EXISTS tici_integration;
USE tici_integration;
DROP TABLE IF EXISTS t1;
CREATE TABLE t1 (id INT PRIMARY KEY, v INT, note VARCHAR(20));
INSERT INTO t1 VALUES (1, 100, 'a'), (2, 200, 'b'), (3, 300, 'c');
UPDATE t1 SET v = v + 1 WHERE id IN (1, 3);
DELETE FROM t1 WHERE id = 2;
SELECT id, v, note FROM t1 ORDER BY id;
